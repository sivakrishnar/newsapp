<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @trailer.id %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">Back</a>
    <a href="<%= url_for :action => :edit, :id => @trailer.object %>" class="ui-btn-right" data-icon="star">Edit</a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">Id</div>
            <div class="itemValue"><%= @trailer.id %></div>
          </li>
      
          <li>
            <div class="itemLabel">Title</div>
            <div class="itemValue"><%= @trailer.title %></div>
          </li>
      
          <li>
            <div class="itemLabel">Url</div>
            <div class="itemValue"><%= @trailer.url %></div>
          </li>
      
          <li>
            <div class="itemLabel">ImageUrl</div>
            <div class="itemValue"><%= @trailer.image_url %></div>
          </li>
      
          <li>
            <div class="itemLabel">CreatedAt</div>
            <div class="itemValue"><%= @trailer.created_at %></div>
          </li>
      
    </ul>
  </div>

</div>
