<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @trailer.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @trailer.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @trailer.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Title</td>
        <td class="itemValue"><%= @trailer.title%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Url</td>
        <td class="itemValue"><%= @trailer.url%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ImageUrl</td>
        <td class="itemValue"><%= @trailer.image_url%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CreatedAt</td>
        <td class="itemValue"><%= @trailer.created_at%></td>
      </tr>
      
    </table>
  </div>

</div>
