<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @headline.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @headline.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="headline[id]" class="fieldLabel">Id</label>
            <input type="text" id="headline[id]" name="headline[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="headline[title]" class="fieldLabel">Title</label>
            <input type="text" id="headline[title]" name="headline[title]" <%= placeholder("Title") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="headline[created-at]" class="fieldLabel">Created-at</label>
            <input type="text" id="headline[created-at]" name="headline[created-at]" <%= placeholder("Created-at") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
