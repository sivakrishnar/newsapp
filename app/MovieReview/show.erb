<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @moviereview.id %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">Back</a>
    <a href="<%= url_for :action => :edit, :id => @moviereview.object %>" class="ui-btn-right" data-icon="star">Edit</a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">Id</div>
            <div class="itemValue"><%= @moviereview.id %></div>
          </li>
      
          <li>
            <div class="itemLabel">Title</div>
            <div class="itemValue"><%= @moviereview.title %></div>
          </li>
      
          <li>
            <div class="itemLabel">Author</div>
            <div class="itemValue"><%= @moviereview.author %></div>
          </li>
      
          <li>
            <div class="itemLabel">Rating</div>
            <div class="itemValue"><%= @moviereview.rating %></div>
          </li>
      
          <li>
            <div class="itemLabel">Content</div>
            <div class="itemValue"><%= @moviereview.content %></div>
          </li>
      
          <li>
            <div class="itemLabel">CreatedAt</div>
            <div class="itemValue"><%= @moviereview.created_at %></div>
          </li>
      
          <li>
            <div class="itemLabel">ImageUrl</div>
            <div class="itemValue"><%= @moviereview.image_url %></div>
          </li>
      
    </ul>
  </div>

</div>
