<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @moviereview.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @moviereview.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @moviereview.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Title</td>
        <td class="itemValue"><%= @moviereview.title%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Author</td>
        <td class="itemValue"><%= @moviereview.author%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Rating</td>
        <td class="itemValue"><%= @moviereview.rating%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Content</td>
        <td class="itemValue"><%= @moviereview.content%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CreatedAt</td>
        <td class="itemValue"><%= @moviereview.created_at%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ImageUrl</td>
        <td class="itemValue"><%= @moviereview.image_url%></td>
      </tr>
      
    </table>
  </div>

</div>
