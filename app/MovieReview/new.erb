<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @moviereview.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @moviereview.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="moviereview[id]" class="fieldLabel">Id</label>
            <input type="text" id="moviereview[id]" name="moviereview[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[title]" class="fieldLabel">Title</label>
            <input type="text" id="moviereview[title]" name="moviereview[title]" <%= placeholder("Title") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[author]" class="fieldLabel">Author</label>
            <input type="text" id="moviereview[author]" name="moviereview[author]" <%= placeholder("Author") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[rating]" class="fieldLabel">Rating</label>
            <input type="text" id="moviereview[rating]" name="moviereview[rating]" <%= placeholder("Rating") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[content]" class="fieldLabel">Content</label>
            <input type="text" id="moviereview[content]" name="moviereview[content]" <%= placeholder("Content") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[created_at]" class="fieldLabel">Created at</label>
            <input type="text" id="moviereview[created_at]" name="moviereview[created_at]" <%= placeholder("Created at") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="moviereview[image_url]" class="fieldLabel">Image url</label>
            <input type="text" id="moviereview[image_url]" name="moviereview[image_url]" <%= placeholder("Image url") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
